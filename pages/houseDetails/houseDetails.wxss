.body{
	background-color: rgb(247, 250, 248)
}
swiper{
	width: 100%;
	height: 500rpx;
}
swiper image{
	width: 100%;
	height: 100%;
}
.box2{
	width: 100%;
	background-color: white;
}
.view2{
	width: 90%;
	margin: auto;
	padding-top: 20rpx;
	padding-bottom: 20rpx;
}
.name{
	font-size: 20px;
}
.view2 image{
	width: 40rpx;
	height: 60rpx;
}
.view2_box{
	display: flex;
	justify-content: space-between;
}
.view2_child{
	margin-top: 40rpx;
	display: flex;
	justify-content: space-between;
	align-items: center;	
}
.view2_child_text{
	margin-left: 20rpx;
}
.view2_child_text text:nth-child(1){
	font-size: 16px;
	color: #535455;
}
.view2_child_text text:nth-child(2){
	font-size: 12px;
	color: #86898b;
}
.view2_child_text2 text:nth-child(1){
	font-size: 16px;
	color: #535455;
}
.view2_child_text2 text:nth-child(2){
	font-size: 12px;
	color: #86898b;
}
/* 3 */
.box3{
	width: 100%;
	background-color: white;
	position: relative;
}
.box3_title{
	font-size: 18px;
	position: absolute;
	top: 20rpx;
	left: 5%;
}
.box3_else{
	display: flex;
	justify-content: center;
	align-items: center;
}
.view3{
	width: 90%;
	margin: auto;
	margin-top: 10rpx;
	display: flex;
	justify-content: flex-end;
	font-size: 18px;
}
.view3_text{
	color:#FF9300;
}
.view3_text text:first-child{
	font-size: 18px;
}
.view3_text text:last-child{
	font-size: 16px;
}
.view3_swiper{
		width: 90%;
		height: 250rpx;
		margin: auto;
		padding-top: 20rpx;
		font-size: 14px;
}
.view3_swiper image{
	width: 50rpx;
	height: 50rpx;
	border-radius: 50%;
}
.view3_swiper_text{
	line-height:50rpx;
	padding-top: 20rpx;
	overflow: hidden;
	text-overflow: ellipsis;
	display: -webkit-box;
	-webkit-line-clamp: 2;
	-webkit-box-orient: vertical;
}
.view3_swiper_user{
	width: 100%;
	padding-top: 20rpx;
	display: flex;
	justify-content: flex-end;
}
/* 4 */
.box4{
	width: 100%;
	background-color: white;
	margin-top: 30rpx;
}
.view4{
	width: 90%;
	margin: auto;
	padding-top: 20rpx;
	padding-bottom: 20rpx;
	margin-top: 10rpx;
	font-size: 18px;
}
.view4 image{
	width: 100rpx;
	height: 100rpx;
	border-radius: 50%;
}
.view4_user{
	padding-top: 30rpx;
	display: flex;
	align-items: center;
}
.view4_text{
	margin-top: 20rpx;
	font-size: 16px;
	color: #535455;
	display: -webkit-box;
	text-overflow: ellipsis;
	-webkit-box-orient: vertical;
	line-height: 55rpx;
}
.ellipsis{
	overflow:hidden;
	-webkit-line-clamp:4;
}
.unellipsis{
	overflow:visible;
	-webkit-line-clamp: 0 ;
}
