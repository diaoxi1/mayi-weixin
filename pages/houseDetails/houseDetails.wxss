/* 8 附近同类房源 */
.box8{
	width: 100%;
	background-color: white;
	margin-top: 30rpx;
}
.view8{
	width: 90%;
	margin: auto;
	padding-top: 20rpx;
	padding-bottom: 20rpx;
	margin-top: 10rpx;
	font-size: 18px;
	overflow: hidden;
}
.view8 movable-area{
	width: 100%;
	height: 450rpx;
}
.view8 movable-view{
	/* width: 250%; */
	height: 450rpx;
	display: flex;
	justify-content: space-around;
	align-content: center;
}
.view8_each{
	width: 550rpx;
	height: 450rpx;
	overflow:hidden;
	display: flex;
	flex-direction: column;
	margin-top: 30rpx;
	position: relative;
}
.view8_each image{
	width: 100%;
	height: 300rpx;
}
.view8_each text:nth-child(2){
	padding: 10rpx 20rpx;
	background-color: rgba(0,0,0,.7);
	color: white;
	position: absolute;
	top: 225rpx;
	left: 0;
}
.view8_name{
	color: #363738;
	font-size: 16px;
	overflow:hidden;
	text-overflow:ellipsis;
	white-space:nowrap;
	margin-top: 20rpx;
	margin-bottom: 20rpx;
}
.view8_each text:nth-child(4){
	color: #86898b;
	font-size: 14px;
	overflow:hidden;
	text-overflow:ellipsis;
	white-space:nowrap;
}
